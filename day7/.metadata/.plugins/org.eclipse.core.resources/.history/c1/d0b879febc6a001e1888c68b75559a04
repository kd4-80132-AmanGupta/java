package com.sunbeam;

public class Program01 {

	public static void main(String[] args) {
		String s1 ; // reference
		s1 = "sunbeam"; // String literals
		//s1 = "karad";
		
		String s2 = "sunbeam";
		
		System.out.println(s1);
		System.out.println(s2);
		
		System.out.println("s1==s2 : "+(s1==s2));
		

	}

}
